
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id String @id @default(uuid())
  firstName String @map("first_name")
  lastName String? @map("last_name")
  email String @unique
  password String
  profileImageUrl String? @map("profile_image_url")
  salt String

  @@map("users")
}
